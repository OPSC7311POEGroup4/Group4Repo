<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContinueWithGoogle">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="433dp"
        android:layout_height="243dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="483dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/googlelogo" />

    <EditText
        android:id="@+id/editTextGoogleEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="62dp"
        android:layout_marginTop="312dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="374dp"
        android:ems="10"
        android:hint="Enter Google Email"
        android:inputType="text"
        android:minHeight="48dp"
        android:textColorHint="#1B1A1A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextGooglePassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="294dp"
        android:ems="10"
        android:hint="Enter password"
        android:inputType="text"
        android:minHeight="48dp"
        android:textColorHint="#272525"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/GoogleContinuebtn"
        android:layout_width="166dp"
        android:layout_height="74dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="493dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="169dp"
        android:backgroundTint="@color/Blue"
        android:text="Continue"
        android:textColor="#DFDADA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>